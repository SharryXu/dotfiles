# Go only recognize the GOPATH variable in the bash profile.
export GOPATH=$HOME/Go:$HOME/Desktop/Projects/example.v2/

export GOROOT=/usr/local/opt/go/libexec
export NVM_DIR="$HOME"/.nvm
export LLVM_PATH=/usr/local/opt/llvm/bin
export PYTHON_CONFIGURE_OPTS="--enable-framework"
export CUSTOM_BIN="$HOME"/.bin
export PACKAGE_BIN=/usr/local/bin
export PACKAGE_SBIN=/usr/local/sbin
export PYENV_ROOT="$HOME/.pyenv"
export PYENV_BIN="$PYENV_ROOT/bin"

export DOTFILES_PATH="$HOME"/Desktop/Projects/dotfiles

export PATH=$CUSTOM_BIN:$PYENV_BIN:$LLVM_PATH:$NVM_DIR:$GOPATH/bin:$PATH:$PACKAGE_BIN:$PACKAGE_SBIN
